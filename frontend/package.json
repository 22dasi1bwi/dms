{
  "name": "codo",
  "description": "Vue.js frontend for CODO",
  "version": "1.0.0",
  "author": "Hauke Brammer <Hauke.Brammer@novatec-gmbh.de>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "ldev": "cross-env NODE_ENV=development webpack --mode=development --hot --watch",
    "build-ldev": "cross-env NODE_ENV=development webpack --mode=development",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build": "tsc --skipLibCheck && cross-env NODE_ENV=production webpack --mode=production --progress --hide-modules"
  },
  "dependencies": {
    "npm": "^6.9.0",
    "vue": "^2.6.10",
    "@vue/cli-service": "^3.9.2",
    "vue-debounce": "^1.3.0",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.2",
    "vuejs-datepicker": "^1.5.4",
    "vuelidate": "^0.7.4",
    "vuex": "^3.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-stage-3": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/bluebird": "^3.5.26",
    "@types/core-js": "^0.9.36",
    "@types/vuelidate": "^0.7.4",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.0.5",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "tailwindcss": "^1.0.4",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
